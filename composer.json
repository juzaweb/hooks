{
    "name": "theanh/laravel-hooks",
    "description": "Add Actions and filters in Laravel like WordPress.",
    "keywords": [
        "laravel hooks",
        "hooks laravel",
        "hooks laravel like wordpress",
        "hooks for laravel"
    ],
    "license": "MIT",
    "authors": [
        {
          "name": "The Anh Dang",
          "email": "dangtheanh16@gmail.com",
          "homepage": "https://www.facebook.com/TheAnhDang.1996/",
          "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": ">=5.3",
        "opis/closure": "^3.5"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "php-coveralls/php-coveralls": "^2.1"
    },
    "autoload": {
        "psr-4": {
          "Theanh\\LaravelHooks\\": "src/"
        }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Theanh\\LaravelHooks\\LaravelHooksServiceProvider"
        ]
      }
    }
}